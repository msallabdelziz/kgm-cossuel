		function convert_date(today) {
			today=new Date(today)
			var dd = today.getDate();

			var mm = today.getMonth()+1; 
			var yyyy = today.getFullYear();
			if(dd<10) 
			{
				dd='0'+dd;
			} 

			if(mm<10) 
			{
				mm='0'+mm;
			} 
			today = yyyy+'-'+mm+'-'+dd;	
			return today;	
		}
